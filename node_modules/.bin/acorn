#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/nxtwave/nodejs/coding-practices/coding-practice-6a/hava-havai-assignmnet/node_modules/.pnpm/acorn@8.12.1/node_modules/acorn/bin/acorn/node_modules:/home/nxtwave/nodejs/coding-practices/coding-practice-6a/hava-havai-assignmnet/node_modules/.pnpm/acorn@8.12.1/node_modules/acorn/bin/node_modules:/home/nxtwave/nodejs/coding-practices/coding-practice-6a/hava-havai-assignmnet/node_modules/.pnpm/acorn@8.12.1/node_modules/acorn/node_modules:/home/nxtwave/nodejs/coding-practices/coding-practice-6a/hava-havai-assignmnet/node_modules/.pnpm/acorn@8.12.1/node_modules:/home/nxtwave/nodejs/coding-practices/coding-practice-6a/hava-havai-assignmnet/node_modules/.pnpm/node_modules:/home/nxtwave/nodejs/coding-practices/coding-practice-6a/hava-havai-assignmnet/node_modules:/home/nxtwave/nodejs/coding-practices/coding-practice-6a/node_modules:/home/nxtwave/nodejs/coding-practices/node_modules:/home/nxtwave/nodejs/node_modules:/home/nxtwave/node_modules:/home/node_modules:/node_modules:/home/nxtwave/nodejs/coding-practices/coding-practice-6a/hava-havai-assignmnet/node_modules/acorn/bin/acorn/node_modules:/home/nxtwave/nodejs/coding-practices/coding-practice-6a/hava-havai-assignmnet/node_modules/acorn/bin/node_modules:/home/nxtwave/nodejs/coding-practices/coding-practice-6a/hava-havai-assignmnet/node_modules/acorn/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../acorn/bin/acorn" "$@"
else 
  exec node  "$basedir/../acorn/bin/acorn" "$@"
fi
