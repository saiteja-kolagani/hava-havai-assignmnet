#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/nxtwave/nodejs/coding-practices/coding-practice-6a/hava-havai-typeorm/node_modules/.pnpm/nodemon@2.0.22/node_modules/nodemon/bin/nodemon.js/node_modules:/home/nxtwave/nodejs/coding-practices/coding-practice-6a/hava-havai-typeorm/node_modules/.pnpm/nodemon@2.0.22/node_modules/nodemon/bin/node_modules:/home/nxtwave/nodejs/coding-practices/coding-practice-6a/hava-havai-typeorm/node_modules/.pnpm/nodemon@2.0.22/node_modules/nodemon/node_modules:/home/nxtwave/nodejs/coding-practices/coding-practice-6a/hava-havai-typeorm/node_modules/.pnpm/nodemon@2.0.22/node_modules:/home/nxtwave/nodejs/coding-practices/coding-practice-6a/hava-havai-typeorm/node_modules/.pnpm/node_modules:/home/nxtwave/nodejs/coding-practices/coding-practice-6a/hava-havai-typeorm/node_modules:/home/nxtwave/nodejs/coding-practices/coding-practice-6a/node_modules:/home/nxtwave/nodejs/coding-practices/node_modules:/home/nxtwave/nodejs/node_modules:/home/nxtwave/node_modules:/home/node_modules:/node_modules:/home/nxtwave/nodejs/coding-practices/coding-practice-6a/hava-havai-typeorm/node_modules/nodemon/bin/nodemon.js/node_modules:/home/nxtwave/nodejs/coding-practices/coding-practice-6a/hava-havai-typeorm/node_modules/nodemon/bin/node_modules:/home/nxtwave/nodejs/coding-practices/coding-practice-6a/hava-havai-typeorm/node_modules/nodemon/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../nodemon/bin/nodemon.js" "$@"
else 
  exec node  "$basedir/../nodemon/bin/nodemon.js" "$@"
fi
