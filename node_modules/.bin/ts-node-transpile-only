#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/nxtwave/nodejs/coding-practices/coding-practice-6a/hava-havai-assignmnet/node_modules/.pnpm/ts-node@10.9.2_0f04c619127a6cbaa0fb685c7eb281ee/node_modules/ts-node/dist/bin-transpile.js/node_modules:/home/nxtwave/nodejs/coding-practices/coding-practice-6a/hava-havai-assignmnet/node_modules/.pnpm/ts-node@10.9.2_0f04c619127a6cbaa0fb685c7eb281ee/node_modules/ts-node/dist/node_modules:/home/nxtwave/nodejs/coding-practices/coding-practice-6a/hava-havai-assignmnet/node_modules/.pnpm/ts-node@10.9.2_0f04c619127a6cbaa0fb685c7eb281ee/node_modules/ts-node/node_modules:/home/nxtwave/nodejs/coding-practices/coding-practice-6a/hava-havai-assignmnet/node_modules/.pnpm/ts-node@10.9.2_0f04c619127a6cbaa0fb685c7eb281ee/node_modules:/home/nxtwave/nodejs/coding-practices/coding-practice-6a/hava-havai-assignmnet/node_modules/.pnpm/node_modules:/home/nxtwave/nodejs/coding-practices/coding-practice-6a/hava-havai-assignmnet/node_modules:/home/nxtwave/nodejs/coding-practices/coding-practice-6a/node_modules:/home/nxtwave/nodejs/coding-practices/node_modules:/home/nxtwave/nodejs/node_modules:/home/nxtwave/node_modules:/home/node_modules:/node_modules:/home/nxtwave/nodejs/coding-practices/coding-practice-6a/hava-havai-assignmnet/node_modules/ts-node/dist/bin-transpile.js/node_modules:/home/nxtwave/nodejs/coding-practices/coding-practice-6a/hava-havai-assignmnet/node_modules/ts-node/dist/node_modules:/home/nxtwave/nodejs/coding-practices/coding-practice-6a/hava-havai-assignmnet/node_modules/ts-node/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../ts-node/dist/bin-transpile.js" "$@"
else 
  exec node  "$basedir/../ts-node/dist/bin-transpile.js" "$@"
fi
