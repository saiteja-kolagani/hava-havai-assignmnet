#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/nxtwave/nodejs/coding-practices/coding-practice-6a/hava-havai-assignmnet/node_modules/.pnpm/crc-32@1.2.2/node_modules/crc-32/bin/crc32.njs/node_modules:/home/nxtwave/nodejs/coding-practices/coding-practice-6a/hava-havai-assignmnet/node_modules/.pnpm/crc-32@1.2.2/node_modules/crc-32/bin/node_modules:/home/nxtwave/nodejs/coding-practices/coding-practice-6a/hava-havai-assignmnet/node_modules/.pnpm/crc-32@1.2.2/node_modules/crc-32/node_modules:/home/nxtwave/nodejs/coding-practices/coding-practice-6a/hava-havai-assignmnet/node_modules/.pnpm/crc-32@1.2.2/node_modules:/home/nxtwave/nodejs/coding-practices/coding-practice-6a/hava-havai-assignmnet/node_modules/.pnpm/node_modules:/home/nxtwave/nodejs/coding-practices/coding-practice-6a/hava-havai-assignmnet/node_modules:/home/nxtwave/nodejs/coding-practices/coding-practice-6a/node_modules:/home/nxtwave/nodejs/coding-practices/node_modules:/home/nxtwave/nodejs/node_modules:/home/nxtwave/node_modules:/home/node_modules:/node_modules:/home/nxtwave/nodejs/coding-practices/coding-practice-6a/hava-havai-assignmnet/node_modules/crc-32/bin/crc32.njs/node_modules:/home/nxtwave/nodejs/coding-practices/coding-practice-6a/hava-havai-assignmnet/node_modules/crc-32/bin/node_modules:/home/nxtwave/nodejs/coding-practices/coding-practice-6a/hava-havai-assignmnet/node_modules/crc-32/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../crc-32/bin/crc32.njs" "$@"
else 
  exec node  "$basedir/../crc-32/bin/crc32.njs" "$@"
fi
