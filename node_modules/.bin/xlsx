#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/nxtwave/nodejs/coding-practices/coding-practice-6a/hava-havai-typeorm/node_modules/.pnpm/xlsx@0.18.5/node_modules/xlsx/bin/xlsx.njs/node_modules:/home/nxtwave/nodejs/coding-practices/coding-practice-6a/hava-havai-typeorm/node_modules/.pnpm/xlsx@0.18.5/node_modules/xlsx/bin/node_modules:/home/nxtwave/nodejs/coding-practices/coding-practice-6a/hava-havai-typeorm/node_modules/.pnpm/xlsx@0.18.5/node_modules/xlsx/node_modules:/home/nxtwave/nodejs/coding-practices/coding-practice-6a/hava-havai-typeorm/node_modules/.pnpm/xlsx@0.18.5/node_modules:/home/nxtwave/nodejs/coding-practices/coding-practice-6a/hava-havai-typeorm/node_modules/.pnpm/node_modules:/home/nxtwave/nodejs/coding-practices/coding-practice-6a/hava-havai-typeorm/node_modules:/home/nxtwave/nodejs/coding-practices/coding-practice-6a/node_modules:/home/nxtwave/nodejs/coding-practices/node_modules:/home/nxtwave/nodejs/node_modules:/home/nxtwave/node_modules:/home/node_modules:/node_modules:/home/nxtwave/nodejs/coding-practices/coding-practice-6a/hava-havai-typeorm/node_modules/xlsx/bin/xlsx.njs/node_modules:/home/nxtwave/nodejs/coding-practices/coding-practice-6a/hava-havai-typeorm/node_modules/xlsx/bin/node_modules:/home/nxtwave/nodejs/coding-practices/coding-practice-6a/hava-havai-typeorm/node_modules/xlsx/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../xlsx/bin/xlsx.njs" "$@"
else 
  exec node  "$basedir/../xlsx/bin/xlsx.njs" "$@"
fi
