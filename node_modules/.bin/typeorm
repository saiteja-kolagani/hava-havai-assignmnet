#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/nxtwave/nodejs/coding-practices/coding-practice-6a/hava-havai-assignmnet/node_modules/.pnpm/typeorm@0.2.45_mysql2@3.10.2+pg@8.12.0/node_modules/typeorm/cli.js/node_modules:/home/nxtwave/nodejs/coding-practices/coding-practice-6a/hava-havai-assignmnet/node_modules/.pnpm/typeorm@0.2.45_mysql2@3.10.2+pg@8.12.0/node_modules/typeorm/node_modules:/home/nxtwave/nodejs/coding-practices/coding-practice-6a/hava-havai-assignmnet/node_modules/.pnpm/typeorm@0.2.45_mysql2@3.10.2+pg@8.12.0/node_modules:/home/nxtwave/nodejs/coding-practices/coding-practice-6a/hava-havai-assignmnet/node_modules/.pnpm/node_modules:/home/nxtwave/nodejs/coding-practices/coding-practice-6a/hava-havai-assignmnet/node_modules:/home/nxtwave/nodejs/coding-practices/coding-practice-6a/node_modules:/home/nxtwave/nodejs/coding-practices/node_modules:/home/nxtwave/nodejs/node_modules:/home/nxtwave/node_modules:/home/node_modules:/node_modules:/home/nxtwave/nodejs/coding-practices/coding-practice-6a/hava-havai-assignmnet/node_modules/typeorm/cli.js/node_modules:/home/nxtwave/nodejs/coding-practices/coding-practice-6a/hava-havai-assignmnet/node_modules/typeorm/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../typeorm/cli.js" "$@"
else 
  exec node  "$basedir/../typeorm/cli.js" "$@"
fi
