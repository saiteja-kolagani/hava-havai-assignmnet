#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/nxtwave/nodejs/coding-practices/coding-practice-6a/hava-havai-typeorm/node_modules/.pnpm/cli-highlight@2.1.11/node_modules/cli-highlight/bin/highlight/node_modules:/home/nxtwave/nodejs/coding-practices/coding-practice-6a/hava-havai-typeorm/node_modules/.pnpm/cli-highlight@2.1.11/node_modules/cli-highlight/bin/node_modules:/home/nxtwave/nodejs/coding-practices/coding-practice-6a/hava-havai-typeorm/node_modules/.pnpm/cli-highlight@2.1.11/node_modules/cli-highlight/node_modules:/home/nxtwave/nodejs/coding-practices/coding-practice-6a/hava-havai-typeorm/node_modules/.pnpm/cli-highlight@2.1.11/node_modules:/home/nxtwave/nodejs/coding-practices/coding-practice-6a/hava-havai-typeorm/node_modules/.pnpm/node_modules:/home/nxtwave/nodejs/coding-practices/coding-practice-6a/hava-havai-typeorm/node_modules:/home/nxtwave/nodejs/coding-practices/coding-practice-6a/node_modules:/home/nxtwave/nodejs/coding-practices/node_modules:/home/nxtwave/nodejs/node_modules:/home/nxtwave/node_modules:/home/node_modules:/node_modules:/home/nxtwave/nodejs/coding-practices/coding-practice-6a/hava-havai-typeorm/node_modules/cli-highlight/bin/highlight/node_modules:/home/nxtwave/nodejs/coding-practices/coding-practice-6a/hava-havai-typeorm/node_modules/cli-highlight/bin/node_modules:/home/nxtwave/nodejs/coding-practices/coding-practice-6a/hava-havai-typeorm/node_modules/cli-highlight/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../cli-highlight/bin/highlight" "$@"
else 
  exec node  "$basedir/../cli-highlight/bin/highlight" "$@"
fi
